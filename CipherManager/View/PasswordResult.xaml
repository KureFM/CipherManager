<UserControl x:Class="CipherManager.View.PasswordResult" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <StackPanel>
        <TextBlock TextBlock.FontSize="18" TextBlock.Foreground="{DynamicResource HighlightBrush}">Generated Password:</TextBlock>
        <TextBox Name="dataBox" VerticalAlignment="Top" TextWrapping="Wrap" IsReadOnly="True" Text="{Binding Data}" />
    </StackPanel>
</UserControl>